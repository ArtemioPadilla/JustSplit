    "@types/uuid": "^9.0.8",
    "chart.js": "^4.4.9",
    "date-fns": "^4.1.0",
    "firebase": "^11.7.3",
    "framer-motion": "^10.16.4",
    "motion": "^12.12.1",
    "next": "^15.3.2",
    "react": "18.3.1",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "18.3.1",
    "react-intersection-observer": "^9.5.2",
    "uuid": "^9.0.1"
